{
    "name": "wp-cli/embed-command",
    "description": "Inspects oEmbed providers, clears embed cache, and more.",
    "type": "wp-cli-package",
    "homepage": "https://github.com/wp-cli/embed-command",
    "support": {
        "issues": "https://github.com/wp-cli/embed-command/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Pascal Birchler",
            "homepage": "https://pascalbirchler.com/"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "WP_CLI\\Embeds\\": "src/"
        },
        "files": [
            "embed-command.php"
        ]
    },
    "require": {},
    "require-dev": {
        "behat/behat": "~2.5",
        "wp-cli/wp-cli": "^1.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "bundled": true,
        "commands": [
            "embed",
            "embed fetch",
            "embed provider list",
            "embed provider match",
            "embed handler list",
            "embed cache clear",
            "embed cache find",
            "embed cache trigger"
        ]
    }
}
